<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="230.0" prefWidth="448.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddNewFriendController">
   <children>
      <Button fx:id="add" layoutX="265.0" layoutY="176.0" mnemonicParsing="false" onAction="#addFriend" text="Submit" />
      <Button fx:id="cancel" layoutX="125.0" layoutY="176.0" mnemonicParsing="false" onAction="#closeWindow" text="Cancel" />
      <Label fx:id="title" layoutX="125.0" layoutY="33.0" text="Add a new Friend">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <TextField fx:id="newUser" layoutX="136.0" layoutY="115.0" prefHeight="21.0" prefWidth="258.0" />
      <Label fx:id="username" layoutX="43.0" layoutY="120.0" text="Enter Name:" />
   </children>
</AnchorPane>
